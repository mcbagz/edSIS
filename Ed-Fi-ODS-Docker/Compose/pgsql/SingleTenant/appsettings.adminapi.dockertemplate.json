{
  "ConnectionStrings": {
    "EdFi_Admin": "host=${ADMIN_POSTGRES_HOST};port=${POSTGRES_PORT};username=${POSTGRES_USER};password=${POSTGRES_PASSWORD};database=EdFi_Admin;",
    "EdFi_Security": "host=${ADMIN_POSTGRES_HOST};port=${POSTGRES_PORT};username=${POSTGRES_USER};password=${POSTGRES_PASSWORD};database=EdFi_Security;",
    "Master": "host=${ADMIN_POSTGRES_HOST};port=${POSTGRES_PORT};username=${POSTGRES_USER};password=${POSTGRES_PASSWORD};database=postgres;"
  },
  "AppSettings": {
    "DefaultPageSizeOffset": "${PAGING_OFFSET:-0}",
    "DefaultPageSizeLimit": "${PAGING_LIMIT:-25}",
    "MultiTenancy": "${MULTITENANCY_ENABLED:-false}"
  },
  "Authentication": {
    "AllowRegistration": "true"
  },
  "SwaggerSettings": {
    "EnableSwagger": "true",
    "DefaultTenant": "${SWAGGER_DEFAULT_TENANT:-tenant1}"
  }
}